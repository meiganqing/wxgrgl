<view class="back">
  <view class="cont"
   bindtap="detailUrl"
    wx:for="{{res}}"
     wx:for-item="item"
      wx:for-index="idx" 
      wx:key="{{item.id}}" 
      data-id="{{item.id}}"
       data-xmlcid="{{item.xmlcid}}"
        data-onlynum="{{item.onlynum}}" 
        data-module="{{item.module}}"
    data-url="{{item.url}}">
    <view class="row">
      <view class="logo">
        <view class="userinfo" id="userinfo">{{item.sendPerson}}</view>
      </view>
      <view class="biaoti">{{item.theme}}</view>
      <view style="" class="time">{{item.sendTime}}</view>
    </view>
    <view class="detail">
      <view>
        <text>发送人:</text>
        <text>{{item.sendPerson}}</text>
      </view>
      <view>
        <text></text>
      </view>
      <view class="shenpi">
        <text>审批编号:</text>
        <text>{{item.onlynum}}</text>
      </view>
    </view>
    <view class="diveline"></view>
  </view>
</view>